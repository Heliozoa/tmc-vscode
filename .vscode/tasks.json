// for the documentation about the tasks.json format
{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "testBuild",
			"type": "npm",
			"script": "watch",
			"problemMatcher": "$tsc-watch",
			"isBackground": true,
			"presentation": {
				"reveal": "never"
			},
			"group": "build"
		},
		{
			"label": "webpackBuild",
			"type": "npm",
			"script": "webpack-dev",
			"problemMatcher": {
				"base": "$tsc-watch",
				"background": {
					"activeOnStart": true,
					"beginsPattern": "^Compilation  startingâ€¦",
					"endsPattern": "^Compilation  finished"
				}
			},
			"isBackground": true,
			"presentation": {
				"reveal": "never"
			},
			"group": "build"
		}
	]
}
