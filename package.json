{
	"name": "tmc-vscode",
	"displayName": "tmc-vscode",
	"description": "TestMyCode extension for Visual Studio Code",
	"version": "0.0.1",
	"license": "MIT",
	"publisher": "tmc-vscode-temporary",
	"repository": "https://github.com/rage/tmc-vscode",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:tmcView"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "tmcView.activateEntry",
				"title": "Activate"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "tmcView.activateEntry",
					"when": "false"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"title": "TestMyCode",
					"id": "tmc",
					"icon": "media/dep.svg"
				}
			]
		},
		"views": {
			"tmc": [
				{
					"id": "tmcView",
					"name": "Menu"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"tslint": "tslint --project ."
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/node-fetch": "^2.5.4",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typemoq": "^2.1.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"client-oauth2": "^4.2.5",
    "handlebars": "^4.7.2",
    "node-fetch": "^2.6.0"
	}
}
